{
  "profiles": {
    "Electron.NET App": {
      "commandName": "Executable",
      "executablePath": "dotnet-electronize",
      "commandLineArgs": "start /args --inspect=9090",
      "workingDirectory": ".",
      "environmentVariables": {
        "HUD_MODE": "electron"
      },
      "applicationUrl": "http://localhost:9999"
    },
    "http": {
      "commandName": "Project",
      //"launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "HUD_MODE": "web"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5093"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7041;http://localhost:5093"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:8612",
      "sslPort": 44384
    }
  }
